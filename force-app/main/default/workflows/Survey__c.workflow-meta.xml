    <rules>
        <fullName>Omnitracs MX Survey Result below threshold</fullName>
        <actions>
            <name>Omnitracs_MX_Survey_Result_below_threshold</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND (2 OR 3 OR 4 OR 5 OR 6 OR 7)</booleanFilter>
        <criteriaItems>
            <field>Survey__c.Business_Unit__c</field>
            <operation>equals</operation>
            <value>Omnitracs MX</value>
        </criteriaItems>
        <criteriaItems>
            <field>Survey__c.Total_Score__c</field>
            <operation>lessOrEqual</operation>
            <value>3</value>
        </criteriaItems>
        <criteriaItems>
            <field>Survey__c.Courtesy_and_Professionalism_of_Analyst__c</field>
            <operation>equals</operation>
            <value>Dissatisfied</value>
        </criteriaItems>
        <criteriaItems>
            <field>Survey__c.Timeliness_of_service_provided__c</field>
            <operation>equals</operation>
            <value>Dissatisfied</value>
        </criteriaItems>
        <criteriaItems>
            <field>Survey__c.Analyst_s_Technical_Skills_Knowledge__c</field>
            <operation>equals</operation>
            <value>Dissatisfied</value>
        </criteriaItems>
        <criteriaItems>
            <field>Survey__c.Quality_of_service_provided__c</field>
            <operation>equals</operation>
            <value>Dissatisfied</value>
        </criteriaItems>
        <criteriaItems>
            <field>Survey__c.Overall_Service_Experience__c</field>
            <operation>equals</operation>
            <value>Dissatisfied</value>
        </criteriaItems>
        <description>Email alert to Customer Support Management team if the survey total score is below the threshold or if the customer was dissatisfied in one (or more) aspect.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
