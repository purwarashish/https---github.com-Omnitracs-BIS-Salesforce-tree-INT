    <rules>
        <fullName>Email Alert on Negative feedback</fullName>
        <actions>
            <name>Email_alert_on_dissatisfied_survey</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Survey__c.Customer_Feedback__c</field>
            <operation>equals</operation>
            <value>Dissatisfied</value>
        </criteriaItems>
        <description>Email Alert on Negative feedback</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Omnitracs MX Survey Request to Speak to Product Team</fullName>
        <actions>
            <name>Omnitracs_MX_Survey_Request_to_Speak_to_Product_Team</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Survey__c.Business_Unit__c</field>
            <operation>equals</operation>
            <value>Omnitracs MX</value>
        </criteriaItems>
        <criteriaItems>
            <field>Survey__c.Request_to_speak_to_Product_Support__c</field>
