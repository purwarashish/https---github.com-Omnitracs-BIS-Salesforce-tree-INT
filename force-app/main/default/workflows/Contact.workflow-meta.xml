    <rules>
        <fullName>Customer Portal Activation Success - Colleague</fullName>
        <actions>
            <name>Customer_Portal_Activation_Email</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Send email to the Customer when he completes the invitation process initiated by the sales person and the account not a customer yet.</description>
        <formula>AND( OR(UnityStatus__c = LOWER(&quot;USER&quot;) , UnityStatus__c = &quot;USER&quot;) , Email &lt;&gt; NULL, UnityInviteSent__c = true, UnityInviteToken__c != &quot;&quot;,OR(Unity_Prov_Type__c = &apos;COLLEAGUE_INVITE&apos;,AND(Account.UnityOnboardStatus__c = &apos;CUSTOMER&apos;,Unity_Prov_Type__c = &apos;SALES_INVITE&apos;)),  $User.Bypass_Workflows__c = false)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Customer Portal Activation Success - Direct Sales</fullName>
        <active>true</active>
        <booleanFilter>1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7</booleanFilter>
        <criteriaItems>
            <field>Contact.UnityStatus__c</field>
            <operation>equals</operation>
            <value>USER</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Email</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.UnityInviteSent__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.UnityInviteToken__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Unity_Prov_Type__c</field>
            <operation>equals</operation>
            <value>NEW_USER</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.UnityOnboardStatus__c</field>
            <operation>notEqual</operation>
            <value>CUSTOMER</value>
        </criteriaItems>
        <criteriaItems>
            <field>User.Bypass_Workflows__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Customer_Portal_Activation_Email</name>
                <type>Alert</type>
            </actions>
            <timeLength>0</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Customer Portal Activation Success - Sales</fullName>
        <active>true</active>
        <description>Send email to the Customer when he completes the invitation process initiated by the sales person and the account not a customer yet.</description>
        <formula>AND( OR(UnityStatus__c = LOWER(&quot;USER&quot;) , UnityStatus__c = &quot;USER&quot;) , Email &lt;&gt; NULL, UnityInviteSent__c = true, UnityInviteToken__c != &quot;&quot;,AND(Unity_Prov_Type__c = &apos;SALES_INVITE&apos;,Account.UnityOnboardStatus__c != &apos;CUSTOMER&apos;), $User.Bypass_Workflows__c = false)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Customer_Portal_Activation_Email</name>
                <type>Alert</type>
            </actions>
            <timeLength>0</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
