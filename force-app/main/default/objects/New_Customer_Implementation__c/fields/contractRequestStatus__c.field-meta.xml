<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>contractRequestStatus__c</fullName>
    <description>The status of the contract request.  A duration more than 14 calendar days is red-flagged.</description>
    <externalId>false</externalId>
    <formula>IF (ISNULL(contractRequestDuration__c), &quot;&quot;, IF (contractRequestDuration__c &gt; 14, IMAGE(&quot;/img/samples/light_red.gif&quot;,&quot;Red&quot;), IMAGE(&quot;/img/samples/light_green.gif&quot;, &quot;Green&quot;)))</formula>
    <label>Contracts Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
