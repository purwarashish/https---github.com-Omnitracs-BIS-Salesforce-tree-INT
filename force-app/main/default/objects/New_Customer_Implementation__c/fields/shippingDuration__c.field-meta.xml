<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>shippingDuration__c</fullName>
    <externalId>false</externalId>
    <formula>IF (ISNULL(orderPlacedDate__c), null, IF (ISNULL(shippedDate__c), Today() -  orderPlacedDate__c , shippedDate__c - orderPlacedDate__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Shipping Duration (days)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
