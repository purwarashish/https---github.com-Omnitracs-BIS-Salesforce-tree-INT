<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ARPU__c</fullName>
    <description>Average Revenue per Unit (ARPU) = Net Revenue / Billed Units</description>
    <externalId>false</externalId>
    <formula>IF (Billed_Units__c = 0, NULL, Net_Revenue__c / Billed_Units__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Average Revenue per Unit (ARPU) = Net Revenue / Billed Units</inlineHelpText>
    <label>ARPU</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
