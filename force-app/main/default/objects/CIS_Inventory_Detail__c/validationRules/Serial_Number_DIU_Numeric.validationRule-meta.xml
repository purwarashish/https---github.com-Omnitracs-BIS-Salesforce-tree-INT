<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Serial_Number_DIU_Numeric</fullName>
    <active>true</active>
    <description>Serial Number DIU should be just numeral</description>
    <errorConditionFormula>NOT(ISNUMBER( Serial_DIU__c ))</errorConditionFormula>
    <errorDisplayField>Serial_DIU__c</errorDisplayField>
    <errorMessage>Serial# DIU should be numeric only</errorMessage>
</ValidationRule>
