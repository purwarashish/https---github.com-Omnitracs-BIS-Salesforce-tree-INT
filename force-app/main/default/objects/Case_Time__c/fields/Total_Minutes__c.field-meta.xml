<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Minutes__c</fullName>
    <description>Calculate the total Minutes using the Hours and Minutes field.</description>
    <externalId>false</externalId>
    <formula>IF(Hours__c &gt; 0, (Hours__c *60) + Minutes__c, Minutes__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Total time spent in minutes. Adds the Hours and Minutes fields together.</inlineHelpText>
    <label>Total Minutes</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
