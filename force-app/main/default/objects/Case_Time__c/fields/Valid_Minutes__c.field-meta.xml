<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valid_Minutes__c</fullName>
    <description>Formula field used in Validation Rule to enforce only specific values in the Minutes field. This field should not be displayed on any page layouts.</description>
    <externalId>false</externalId>
    <formula>CASE(Minutes__c, 0, &quot;True&quot;, 15, &quot;True&quot;, 30, &quot;True&quot;, 45, &quot;True&quot;, &quot;False&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Valid Minutes</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
